      <members>
        <member>testassembly.class2</member>
        <member>testassembly.class3</member>
      </members>
      <namespaces>
        <namespace>testassembly.somenamespace1</namespace>
        <namespace>testassembly.somenamespace2</namespace>
      </namespaces>
    </test_assembly>
    <test_assembly name="C:\my\anothernunitassembly.dll" />
  </runner>
  <runner id="another">
    <test_assembly name="C:\my\other\testassembly.dll" />
  </runner>
</run>